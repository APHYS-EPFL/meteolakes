{
  "name": "lake-view",
  "version": "1.0.0",
  "description": "Forecasting hydrodynamic model of Lake Geneva",
  "main": "app.js",
  "scripts": {
    "build": "browserify app.js -d | exorcist bundle.js.map > bundle.js",
    "uglify": "uglifyjs bundle.js --in-source-map bundle.js.map -o bundle.min.js --source-map bundle.min.js.map",
    "start": "npm run build && npm run uglify"
  },
  "dependencies": {
    "angular": "^1.5.0"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "exorcist": "^0.4.0",
    "source-map-support": "^0.4.0",
    "uglify-js": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://zoller@git.epfl.ch/repo/lake-view"
  },
  "author": "APHYS",
  "license": "MIT"
}
